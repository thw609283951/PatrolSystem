<?xml version="1.0" encoding="utf-8"?>
<com.supersit.common.widget.MyLinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:hyphenate="http://schemas.android.com/apk/res-auto"
    style="@style/CommonFragment"
    android:orientation="vertical">
    <TextView
        android:id="@+id/fillStatusBarView"
        style="@style/fitsystem"
        android:layout_width="match_parent"
        android:layout_height="20dp"/>
    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <com.supersit.common.widget.ProgressWebView
            android:id="@+id/webView"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <include layout="@layout/base_error_page"/>
    </FrameLayout>
</com.supersit.common.widget.MyLinearLayout>
